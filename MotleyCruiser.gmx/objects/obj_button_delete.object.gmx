            <string>/// Delete ship
var ship = obj_ship_editor.loaded_ship_index;
if(ship!=noone){

    if(ship&lt;0 || ship&gt;array_length_1d(obj_ship_editor.ship_library)){
        show_message("Invalid ship index.");
        exit;
    }


    if show_question("Are you sure you want to delete?")
    {
        var tmp_array = noone;
        for(var i=0;i&lt; array_length_1d(obj_ship_editor.ship_library); i++){
            if(i!=ship){
                tmp_array = scr_push_array(tmp_array,obj_ship_editor.ship_library[i]);
            }
        }
        obj_ship_editor.ship_library = tmp_array;
        scr_write_array(obj_ship_editor.ship_library,"libraries","ships",DATA_FILE);
        scr_clear_editor();
    }

}else{
    show_message("No ship is loaded.");

}
</string>
