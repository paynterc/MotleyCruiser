enum ship_function {
    trader,
    military,
    transport,
    miner,
    courier
}

enum ship_types {
    ship=0,
    drone=1,
    platform=2
}

enum ship_class {
    drone=0,
    platform=1,
    fighter=2,
    corvette=3,
    destroyer=4,
    frigate=5,
    cruiser=6,
    battleship=7,
