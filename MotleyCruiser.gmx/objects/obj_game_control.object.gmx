            <string>/// Pause game,  Debug commands
if(scr_user_input("pause")){   
    if(room!=room_init &amp;&amp; room!=rm_main_menu){
        global.paused = !global.paused;
        if(global.paused){
            scr_gui_set_context(obj_pause_menu);
        }else{
            scr_gui_return_context();
        }
    }   
}

if(scr_user_input("inventory_room")){
    room_goto(rm_galley);
    exit;
}
if(scr_user_input("test_room")){
    // shift, control, T
    room_goto(rm_bullettest);
    exit;
}



/****
Debug navigation commands
****/
