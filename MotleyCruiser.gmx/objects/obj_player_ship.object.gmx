        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Initialize
faction = FACTION_PLAYER;


ship_data = scr_ship();
modules = ship_data[SHIP_MODULES];

scaffold = global.scaffolds[ship_data[SHIP_SCAFFOLD_INDEX]];
guns = noone;

var s = scaffold[SCAFFOLD_SCALE];
var mag = scaffold[SCAFFOLD_ROWS];// Assume scaffolds are of equal height and width

// Set offsets for each module according to their scaffold coordinates
for(var m=0; m&lt;array_length_1d(modules); m++){
    var module = modules[m];
    var coords = module[MODULE_COORDS];
    var r = coords[0];
    var c = coords[1];
    
    var offsets = noone;
    var gunoffsets = noone;
    
    offsets = scr_scaffold_offsets(s,mag,r,c,0);   
    module[MODULE_OFFSETS] = offsets;
    
    var guntrack=noone;
    guntrack[0]=0;//timer
    guntrack[1]=noone;//module index
    if(module[MODULE_TYPE]=="gun"){
        gunoffsets = scr_scaffold_offsets(s,mag,r,c,sprite_get_width(module[MODULE_SPRITE_INDEX])/2);
        guntrack[1] = m;
        guns = scr_push_array(guns,guntrack);
        module[MODULE_GUN_OFFSETS]=gunoffsets;
    }
    modules[m]=module;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="2" enumb="0">
