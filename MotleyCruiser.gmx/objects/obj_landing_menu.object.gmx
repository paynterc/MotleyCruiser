nameTextY = obj_landing_image.y + (obj_landing_image.sprite_height/2)+nameFontLS;
landingTextY = nameTextY+nameFontLS;
obj_landing_text.y = landingTextY;
with(obj_landing_text){
    event_user(0);
}
