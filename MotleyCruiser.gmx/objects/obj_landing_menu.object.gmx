    var spr=L[LOC_SPRITE];
    with(obj_landing_image){
        sprite_index = spr;
        image_index = L[LOC_SPRITE];
        var newScale = floor((128/sprite_width) * 100);
        image_xscale=newScale / 100;
        image_yscale=newScale / 100;
    }
}else if(L[LOC_TYPE]==GX_STATION){
    var stationData = L[LOC_IMAGE_DATA];
    if(is_array(stationData)){
        var station_modules = scr_station_modules();
        var mData =  stationData[0];          
        var sprSection = station_modules[mData[0]];
        var sprIdx = sprSection[mData[1]];
        with(obj_landing_image){
            sprite_index = sprIdx;
            image_speed = 1/(room_speed * 2);
        }

    }
}else{
    // no image?
}
