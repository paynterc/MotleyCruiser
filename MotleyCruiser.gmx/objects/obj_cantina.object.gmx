            <kind>1</kind>
            <string>/// SET UP THE MAP
// Run this in the create step of a child of obj_room. That way the grids should be automatically cleaned up at room end.
view_visible[0] = true;
view_enabled = true;
view_wview[0]=640;
view_hview[0]=360;
view_hborder[0]=160;
view_vborder[0]=90;
view_hspeed[0]=-1;
view_vspeed[0]=-1;

// Resize
room_width=(CELL_WIDTH/16)*720;// why divide by 16?
room_height = (CELL_HEIGHT/16)*720;
var width = room_width div CELL_WIDTH;
var height = room_height div CELL_HEIGHT;
var cx = width div 2;
var cy = height div 2;

// Create the DS global.grid
global.grid = ds_grid_create(width, height);

// Create the pathfinding global.grid
global.grid_path = mp_grid_create(0,0,width,height,CELL_WIDTH,CELL_HEIGHT);

// Fill the global.grid with voids
ds_grid_set_region(global.grid, 0, 0, width-1, height-1, VOID);

/// Get dock data
// 0 = dock, 1 = cantina
var locidx = global.landed_on[LOC_INDEX];
var sector = global.landed_on[LOC_SECTOR];
var roomData = scr_gx_get_by_key(sector[0],sector[1],locidx+"-cantina");

if(is_undefined(roomData)){
    // generate new dock data
    roomData = scr_loc_generate_cantina();
    scr_gx_save_by_key(sector[0],sector[1],locidx+"-cantina",roomData);
}

// Instantiate the room
scr_loc_instantiate_cantina(roomData);

scr_add_wall_tiles(height,width);
</string>
